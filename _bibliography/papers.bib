	journal = {Nature Cardiovascular Research},
	author = {Jiang, Meijie and Tian, Sifan and Liu, Shuzhen and Wang, Yuting and Guo, Xinbiao and Huang, Tao and Lin, Xihong and Belsky, Daniel W. and Baccarelli, Andrea A. and Gao, Xu},
	month = mar,
