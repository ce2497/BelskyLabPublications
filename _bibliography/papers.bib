	keywords = {Female, Humans, Male, Adult, Adolescent, Risk Factors, Black or African American, White People, Age Factors, Health Surveys, Longitudinal Studies, Sex Factors, Health Behavior, Alcoholism, Alcohol Drinking, Black People, California, Coronary Artery Disease},
