	author = {Whitman, Ethan T. and Ryan, Calen P. and Abraham, Wickliffe C. and Addae, Angela and Corcoran, David L. and Elliott, Maxwell L. and Hogan, Sean and Ireland, David and Keenan, Ross and Knodt, Annchen R. and Melzer, Tracy R. and Poulton, Richie and Ramrakha, Sandhya and Sugden, Karen and Williams, Benjamin S. and Zhou, Jiayi and Hariri, Ahmad R. and Belsky, Daniel W. and Moffitt, Terrie E. and Caspi, Avshalom},
	month = apr,
	year = {2024},
	keywords = {Epigenetics, Brain structure, Magnetic resonance imaging, Neurodegeneration},
