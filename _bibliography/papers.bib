	pmcid = {PMC6314676},
	keywords = {polygenic score, educational attainment, GxE, schools},
	pages = {513--540},
	file = {Full Text:/Users/ce2497/Zotero/storage/62VDRH44/Trejo et al. - 2018 - Schools as Moderators of Genetic Associations with Life Course Attainments Evidence from the WLS an.pdf:application/pdf},
}

@article{marioni_association_2018,
	title = {Association of facial ageing with {DNA} methylation and epigenetic age predictions},
