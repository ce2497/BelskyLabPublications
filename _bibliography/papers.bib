	pmcid = {PMC10842850},
	keywords = {Humans, Aging, Biological aging, Black or African American, Mortality, Morbidity, Health Status Disparities, White, Life Change Events, Life course, Racialized disparities, Weathering},
	pages = {1815--1841},
	file = {Full Text PDF:/Users/ce2497/Zotero/storage/EZWX7RWH/Boen et al. - 2023 - Patterns and Life Course Determinants of Black-White Disparities in Biological Age Acceleration A D.pdf:application/pdf},
