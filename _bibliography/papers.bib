	month = feb,
	year = {2025},
	pages = {376--385},
	file = {Full Text PDF:/Users/ce2497/Zotero/storage/CDM7GNZB/Bischoff-Ferrari et al. - 2025 - Individual and additive effects of vitamin D, omega-3 and exercise on DNA methylation clocks of biol.pdf:application/pdf},
