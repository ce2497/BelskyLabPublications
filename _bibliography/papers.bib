	keywords = {Female, Humans, Male, Middle Aged, Adult, Cohort Studies, Adolescent, Child, Young Adult, Aging, Nutrition Surveys, Cause of Death, Cancer Survivors, Frailty, Neoplasms},
	pages = {731--741},
