	pmcid = {PMC3813310},
	keywords = {Female, Humans, Male, Adult, Infant, Newborn, Obesity, Prospective Studies, Adolescent, Child, Child, Preschool, Young Adult, Intelligence Tests, Age Factors, New Zealand, cognitive aging, life course, obesity, Intelligence, Body Weights and Measures, intellectual decline, intelligence quotient, IQ decline, longitudinal study, metabolic syndrome, Obesity, Morbid, Pediatric Obesity},
