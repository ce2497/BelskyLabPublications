	urldate = {2025-12-05},
	journal = {Aggression and Violent Behavior},
	author = {Barnes, J.C. and Liu, Hexuan and Motz, Ryan T. and Tanksley, Peter T. and Kail, Rachel and Beckley, Amber L. and Belsky, Daniel W. and Domingue, Benjamin W. and Moffitt, Terrie E. and Pratt, Travis C. and Wertz, Jasmin},
	month = nov,
