	pmid = {34543722},
	pmcid = {PMC8720865},
	keywords = {Body Mass Index, Humans, Genetic Predisposition to Disease, Aging, Caloric Restriction, Energy Intake, Longevity, Alpha-Ketoglutarate-Dependent Dioxygenase FTO, Eating behaviors, Fat mass and obesity-associated gene, Genotype, Metabolic adaptation, Personalized nutrition, Polymorphism, Single Nucleotide},
	pages = {111555},
	file = {Accepted Version:/Users/ce2497/Zotero/storage/MDGAZEHI/Dorling et al. - 2021 - Association between the FTO rs9939609 single nucleotide polymorphism and dietary adherence during a.pdf:application/pdf},
