	journal = {eLife},
	author = {Belsky, Daniel W and Caspi, Avshalom and Corcoran, David L and Sugden, Karen and Poulton, Richie and Arseneault, Louise and Baccarelli, Andrea and Chamarti, Kartik and Gao, Xu and Hannon, Eilis and Harrington, Hona Lee and Houts, Renate and Kothari, Meeraj and Kwon, Dayoon and Mill, Jonathan and Schwartz, Joel and Vokonas, Pantel and Wang, Cuicui and Williams, Benjamin S and Moffitt, Terrie E},
	editor = {Deelen, Joris and Tyler, Jessica K and Suderman, Matthew and Deelen, Joris},
	month = jan,
