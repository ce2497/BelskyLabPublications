	keywords = {Humans, Health Status, Aging, Longevity, Healthy Aging, World Health Organization},
	pages = {e789--e796},
	file = {Full Text:/Users/ce2497/Zotero/storage/R5MYQS9Q/Bautmans et al. - 2022 - WHO working definition of vitality capacity for healthy longevity monitoring.pdf:application/pdf},
