	journal = {The Lancet. Respiratory Medicine},
	author = {Belsky, Daniel W. and Sears, Malcolm R. and Hancox, Robert J. and Harrington, Honalee and Houts, Renate and Moffitt, Terrie E. and Sugden, Karen and Williams, Benjamin and Poulton, Richie and Caspi, Avshalom},
	month = aug,
	year = {2013},
	pmid = {24429243},
