@article{gladyshev_disagreement_2024,
	title = {Disagreement on foundational principles of biological aging},
	volume = {3},
	issn = {2752-6542},
	url = {https://doi.org/10.1093/pnasnexus/pgae499},
	doi = {10.1093/pnasnexus/pgae499},
	abstract = {To gain insight into how researchers of aging perceive the process they study, we conducted a survey among experts in the field. While highlighting some common features of aging, the survey exposed broad disagreement on the foundational issues. What is aging? What causes it? When does it begin? What constitutes rejuvenation? Not only was there no consensus on these and other core questions, but none of the questions received a majority opinion—even regarding the need for consensus itself. Despite many researchers believing they understand aging, their understanding diverges considerably. Importantly, as different processes are labeled as “aging” by researchers, different experimental approaches are prioritized. The survey shed light on the need to better define which aging processes this field should target and what its goals are. It also allowed us to categorize contemporary views on aging and rejuvenation, revealing critical, yet largely unanswered, questions that appear disconnected from the current research focus. Finally, we discuss ways to address the disagreement, which we hope will ultimately aid progress in the field.},
	number = {12},
	urldate = {2025-12-04},
	journal = {PNAS Nexus},
	author = {Gladyshev, Vadim N and Anderson, Benjamin and Barlit, Hanna and Barré, Benjamin and Beck, Samuel and Behrouz, Bahareh and Belsky, Daniel W and Chaix, Amandine and Chamoli, Manish and Chen, Brian H and Cheng, Kaiyang and Chuprin, Jane and Churchill, Gary A and Cipriano, Andrea and Colville, Alex and Deelen, Joris and Deigin, Yuri and Edmonds, KeHuan K and English, Bradley W and Fang, Ruogu and Florea, Michael and Gershteyn, Iosif M and Gill, Diljeet and Goetz, Laura H and Gorbunova, Vera and Griffin, Patrick T and Horvath, Steve and Borch Jensen, Martin and Jin, Xin and Jovanovska, Sara and Kajderowicz, Kathrin M and Kasahara, Tomoko and Kerepesi, Csaba and Kulkarni, Subhash and Labunskyy, Vyacheslav M and Levine, Morgan E and Libert, Sergiy and Lu, J Yuyang and Lu, Yuancheng Ryan and Marioni, Riccardo E and McCoy, Brianah M and Mitchell, Wayne and Moqri, Mahdi and Nasirian, Farzaneh and Niimi, Peter and Oh, Hamilton Se-Hwee and Okundaye, Brian and Parkhitko, Andrey A and Peshkin, Leonid and Petljak, Mia and Poganik, Jesse R and Pridham, Glen and Promislow, Daniel E L and Prusisz, Weronika and Quiniou, Margaux and Raj, Ken and Richard, Daniel and Ricon, Jose Luis and Rutledge, Jarod and Scheibye-Knudsen, Morten and Schork, Nicholas J and Seluanov, Andrei and Shadpour, Michael and Shindyapina, Anastasia V and Shuken, Steven R and Sivakumar, Sruthi and Stoeger, Thomas and Sugiura, Ayumu and Sutton, Nadia R and Suvorov, Alexander and Tarkhov, Andrei E and Teeling, Emma C and Trapp, Alexandre and Tyshkovskiy, Alexander and Unfried, Maximilian and Ward-Caviness, Cavin K and Yim, Sun Hee and Ying, Kejun and Yunes, Jeffrey and Zhang, Bohan and Zhavoronkov, Alex},
