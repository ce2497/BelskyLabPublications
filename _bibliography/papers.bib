	keywords = {Body Mass Index, Body Weight, Female, Humans, Male, Obesity, Cohort Studies, Black or African American, Age Factors, Longitudinal Studies, Sex Factors, body mass index, Ethnicity, Health Status Disparities, Hispanic or Latino, Educational Status, age trajectory, Body-Weight Trajectory, life course, obesity, Race Factors, social disparities},
