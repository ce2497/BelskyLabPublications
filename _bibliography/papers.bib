	note = {Publisher: Nature Publishing Group},
	keywords = {Endocrine system and metabolic diseases, Cardiovascular diseases, Disease prevention},
	pages = {332--342},
	file = {Submitted Version:/Users/ce2497/Zotero/storage/YC8D9X7V/Jiang et al. - 2024 - Accelerated biological aging elevates the risk of cardiometabolic multimorbidity and mortality.pdf:application/pdf},
